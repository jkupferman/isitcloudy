<%# page_title "Is [website] cloudy?" %>

<div id="question">
<% form_for @website do |f| %>
  <div>
    <%= flash_messages %>
  </div>
  <div>
    <%#= f.error_messages %>
  </div>
  <div id="question">
    <h2>Is</h2><%= f.text_field :url, :size => 20, :value => "google.com", :autocomplete => "off", :onclick => "this.value=''; this.onclick=''" %><h2>cloudy?</h2>
  </div>
  <div id="submit">
    <%= submit_tag "Submit" %>
  </div>
<% end %>
</div>
